    "casual": "^1.6.2",
    "copy-webpack-plugin": "^11.0.0",
    "cors": "^2.8.5",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "express-graphql": "^0.12.0",
    "file-loader": "^6.2.0",
    "graphql": "^16.5.0",
    "html-webpack-plugin": "^5.5.0",
    "immer": "^9.0.15",
    "jest": "^28.1.3",
