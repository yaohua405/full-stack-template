{
  "scripts": {
    "build": "yarn s-clean && yarn build:prod",
    "build:dev": "env -d webpack --mode development",
    "build:prod": "env -p webpack --mode production --progress",
    "gen": "yarn ts-node scripts/schema-compile.ts",
    "start": "env run-p -r start:*",
    "start:build": "yarn build:dev -- --watch",
    "start:server": "nodemon \"./dist/server.js\" --watch \"./dist\" -d 2",
    "launch": "yarn build:dev --progress && node dist/server.js",
    "compose": "yarn s-clean && env -docker yarn build:prod && yarn compose:all",
    "compose:all": "env -docker docker compose up --detach --build",
    "compose:db": "env -docker docker compose up db --detach --build",
    "test": "jest"
  },
  "private": "true",
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
